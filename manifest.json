{
    "manifest_version": 3,
    "name": "Get Data YAMAHA",
    "description": "Sync data from Yamaha CMS to Store CMS",
    "version": "1.2.1",
    "action": {
      "default_icon": "logo-icon.png",
      "default_popup": "index.html"
    },
    "permissions": [
      "storage"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://www.a1-asia.com/*",
          "https://*.a1-asia.com/*"
        ],
        "js": [
          "response.js",
          "index.js"
        ],
        "all_frames": true
      }
    ]
  }
  